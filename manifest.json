{
  "manifest_version": 2,
  "name": "ASM-Fill",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "coffee.png"
  },
  "web_accessible_resources": [
    "elements.html",
    "elements_style.css"
  ]
}
